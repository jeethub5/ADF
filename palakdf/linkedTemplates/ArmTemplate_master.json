{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "palakdf"
		},
		"ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS'"
		},
		"AzureSQLDBService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDBService'"
		},
		"AzureSqlPalakDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlPalakDB'"
		},
		"MovefilesLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MovefilesLS'"
		},
		"SQLDW_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLDW'"
		},
		"ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://palakdatalake.dfs.core.windows.net"
		},
		"MoviesGithub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/djpmsft/adf-ready-demo/master/moviesDB.csv"
		},
		"ssisIR_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "palakserver.database.windows.net"
		},
		"ssisIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "test"
		},
		"ssisIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_accountKey": {
						"value": "[parameters('ADLS_accountKey')]"
					},
					"AzureSQLDBService_connectionString": {
						"value": "[parameters('AzureSQLDBService_connectionString')]"
					},
					"AzureSqlPalakDB_connectionString": {
						"value": "[parameters('AzureSqlPalakDB_connectionString')]"
					},
					"MovefilesLS_connectionString": {
						"value": "[parameters('MovefilesLS_connectionString')]"
					},
					"SQLDW_connectionString": {
						"value": "[parameters('SQLDW_connectionString')]"
					},
					"ADLS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_properties_typeProperties_url')]"
					},
					"MoviesGithub_properties_typeProperties_url": {
						"value": "[parameters('MoviesGithub_properties_typeProperties_url')]"
					},
					"ssisIR_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('ssisIR_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"ssisIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('ssisIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"ssisIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('ssisIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}